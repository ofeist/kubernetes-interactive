apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: httpd-ingress
  namespace: default
spec:
  ingressClassName: nginx  # Uverite se da ovo odgovara va≈°em ingress kontroleru
  rules:
    - host: baba.local
      http:
        paths:
          - path: /
            pathType: Prefix
            backend:
              service:
                name: httpd-service
                port:
                  number: 80
